<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="doctor_model_tree" model="ir.ui.view">
            <field name="name">doctor.model.tree ()</field>
            <field name="model">doctor.model</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

     <record id="doctor_model_form" model="ir.ui.view">
            <field name="name">doctor.model.form ()</field>
            <field name="model">doctor.model.form</field>
            <field name="arch" type="xml">
                <form>
                    <header></header>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

     <record id="doctor_model_act_window" model="ir.actions.act_window">
            <field name="name">Doctor</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">doctor.model</field>
            <field name="view_mode">tree,form</field>
        </record>
</odoo>
